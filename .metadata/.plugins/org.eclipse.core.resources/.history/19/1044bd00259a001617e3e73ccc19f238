var ebay = angular.module('ebay', ['ngRoute']);

ebay.config(['$routeProvider',function($routeProvider) {
    $routeProvider
    .when("/", {
        templateUrl : "templates/Home.html"
    })
    .when("/login", {
        templateUrl : "templates/login.html"
    })
    .otherwise({
            redirectTo: '/'
        });
}]);