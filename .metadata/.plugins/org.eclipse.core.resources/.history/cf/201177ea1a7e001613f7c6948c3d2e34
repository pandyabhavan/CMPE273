package com.cmpe273.homework1.java;

public class MultiThreadingRabbit implements Runnable
{
	Thread t;
	public MultiThreadingRabbit()
	{
		t = new Thread(this);
		t.setPriority(10);
		t.start();
	}
	
	public void run()
	{
		for(int i=0;i<100;i++)
		{
			System.out.println("Rabbit Covered: "+i);
		}
		System.out.println("Rabbit Won");
	} 
}