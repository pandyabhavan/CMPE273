var ebay = angular.module('ebay', ['ngRoute']);

ebay.config(function($routeProvider) {
    $routeProvider
    .when("/", {
        templateUrl : "templates/Home.html"
    })
    .when("/login", {
        templateUrl : "templates/login.html"
    })
    .when('/search', {
        templateUrl : "templates/search.html"
    })
    .when('/cart', {
        templateUrl : "templates/cart.html"
    })
    .when('/search', {
        templateUrl : "templates/search.html"
    })
    .otherwise({
        redirectTo: '/'
    });
});